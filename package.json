{
  "name": "@deusfinance/synchronizer-sdk",
  "license": "MIT",
  "version": "1.0.5",
  "description": "An SDK to interact with the Synchronizer to build financial applications on top of DEUS Finance.",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/deusfinance/synchronizer-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/deusfinance/synchronizer-sdk/issues"
  },
  "homepage": "https://github.com/deusfinance/synchronizer-sdk#readme",
  "keywords": [
    "deus",
    "synchronizer",
    "sdk"
  ],
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "index.js",
      "src/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "inline-json-import",
        {}
      ],
      [
        "@babel/plugin-transform-runtime",
        {
          "regenerator": true
        }
      ]
    ]
  },
  "scripts": {
    "start": "npm run prod",
    "build": "npm-run-all clean transpile",
    "main": "node ./dist",
    "dev": "NODE_ENV=development npm-run-all build main",
    "example": "npx babel-node --inspect --presets @babel/preset-env -- ./example/index.js --inspect",
    "prod": "NODE_ENV=production npm-run-all build main",
    "transpile": "babel ./src --out-dir dist",
    "clean": "rimraf dist",
    "test": "jest",
    "watch:dev": "nodemon"
  },
  "dependencies": {
    "isomorphic-unfetch": "^3.1.0"
  },
  "engines": {
    "node": ">=14"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/node": "^7.16.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "babel-jest": "^27.3.1",
    "babel-plugin-inline-json-import": "^0.3.2",
    "core-js": "^3.19.0",
    "jest": "^27.3.1",
    "nodemon": "^2.0.14",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  }
}
